{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "lib": [
      "ES5","ES6","DOM","DOM.Iterable"
    ],
    "moduleResolution": "node",
    "strict": true,
    "alwaysStrict": true,
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "outDir": "dist",
    "listEmittedFiles": true,
  },
  "include": [
    "src/**/*"
  ]
}
